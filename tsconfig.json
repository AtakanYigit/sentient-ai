{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist",
        "rootDir": "./",
		"forceConsistentCasingInFileNames": true,
		"strict": false,
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"moduleDetection": "auto",
		"sourceMap": true,
		"removeComments": true,
		"resolveJsonModule": true
    }
} 